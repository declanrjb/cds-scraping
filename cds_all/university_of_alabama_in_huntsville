<script>
	var categories = ["Uncategorised"];
	var tags = [];
	var details = '<div class="details"><h3>Details</h3><div class="filedin"><p>FILED IN</p>';
	jQuery.each(categories, function(index, value) {
		details += '<span style="margin-bottom: 6px">' + value + '</span> ';
	});
	details += '</div>';
	if (tags.length) {
		details += '<div class="tagged"><p>TAGGED</p>';
		jQuery.each(tags, function(index, value) {
			details += '<span style="margin-bottom: 6px">' + value.alias + '</span> ';
		});
		details += '</div>';
	}
	details += '</div>';
	jQuery(document).ready(function() {
		var $row = jQuery('.call-to-action').parent().parent();
		if ($row.hasClass('item-page')) {
			$row = jQuery('.call-to-action');
			jQuery('<div class="col-sm-6" style="margin-bottom: 20px">' + details + '</div>').insertBefore($row);
			$row.wrap('<div class="col-sm-6"></div>');
		} else if ($row.attr('id') == 'articleBody') {
			$row = jQuery('.call-to-action');
			jQuery('<div class="col-sm-6" style="margin-bottom: 20px">' + details + '</div>').insertBefore($row);
			$row.wrap('<div class="col-sm-6"></div>');
		} else {
			$row.children('.col-sm-6').removeClass('col-sm-6').addClass('col-sm-4');
			$row.prepend('<div class="col-sm-4">' + details + '</div>');
		}
	});
</script>
